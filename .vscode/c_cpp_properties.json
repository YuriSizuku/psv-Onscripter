{
    "env" : {
        "vitasdk": "d:/AppExtend/SDK/psvsdk"
    },
    "configurations": [
        {
            "name": "vita",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/src/",
                "${vitasdk}/include/onsjh/simd/**",
                "${vitasdk}/arm-vita-eabi/include/SDL2/**",
                "${vitasdk}/arm-vita-eabi/include/**",
                "${vitasdk}/share/gcc-arm-vita-eabi/samples/common/**"
            ],
            "defines": [
                "PSV",
                "PSV_MODULE",
                "USE_PARALLEL",
                "USE_SIMD_ARM_NEON"
            ],
            "compilerPath": "${vitasdk}/bin/arm-vita-eabi-gcc",
            "compilerArgs": [],
            "cStandard": "c99",
            "cppStandard": "c++11",
            "intelliSenseMode": "linux-gcc-arm"
        }
    ],
    "version": 4
}